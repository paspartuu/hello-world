import random


def read_drewn_numbs(): #arvotut luvut
  drawn_numbs = []
  drawn_numbs_set = set()
  while len(drawn_numbs) < 70:
    x = randint(1,70)
    if x not in drawn_numbs:
      drawn_numbs.append(x)
      drawn_numbs_set.add(x)
  
  

  
